{
	"name": "tsfc",
	"version": "1.0.0",
	"description": "Let Node execute TypeScript files directly, support SWC, esbuild, and tsc compilers",
	"license": "MIT",
	"author": "Kaciras <Kaciras@outlook.com>",
	"type": "module",
	"exports": {
		".": "./lib/loader.js",
		"./register": "./lib/register.js"
	},
	"files": [
		"lib/*.{js,d.ts}"
	],
	"scripts": {
		"build": "tsc",
		"test": "node --import ./lib/register.js --test test/*.spec.ts"
	},
	"dependencies": {
		"tsconfck": "^3.0.3"
	},
	"devDependencies": {
		"@kaciras/eslint-config-core": "^3.0.1",
		"@kaciras/eslint-config-typescript": "^3.0.1",
		"@types/node": "^20.12.12",
		"@types/tar-fs": "^2.0.4",
		"esbench": "^0.2.1",
		"eslint": "^9.3.0",
		"tar-fs": "^3.0.6",
		"typescript": "^5.4.5"
	},
	"peerDependencies": {
		"@swc/core": "^1.5.5",
		"esbuild": "^0.21.1"
	},
	"peerDependenciesMeta": {
		"esbuild": {
			"optional": true
		},
		"@swc/core": {
			"optional": true
		}
	}
}
