{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"paths": {
			"_pkg": [
				"./types.d.ts"
			],
			"exact-*": [
				"../fixtures/invalid.js"
			],
			"exact-match": [
				"INVALID",
				"../fixtures/module.ts"
			],
			"prefix/*": [
				"../fixtures/*"
			],
			"*/suffix": [
				"../fixtures/*"
			],
			"@app/*/index.ts": [
				"../fixtures/*.ts"
			],
		}
	}
}
